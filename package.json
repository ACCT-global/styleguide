{
  "name": "@vtex/styleguide",
  "version": "8.70.4",
  "scripts": {
    "test": "react-scripts test --env=jsdom --testPathIgnorePatterns=\"<rootDir>/react/node_modules\" --moduleDirectories=node_modules --testMatch=\"<rootDir>/react/**/?(*.)+(spec|test).js\" --setupTestFrameworkScriptFile=\"<rootDir>/setupTests.js\"",
    "test:codemod": "jest codemod",
    "type-check": "tsc --noEmit",
    "predeploy": "npm run styleguide:build",
    "deploy": "gh-pages -d styleguide",
    "styleguide": "styleguidist server",
    "start": "styleguidist server",
    "styleguide:build": "styleguidist build && echo 'styleguide.vtex.com' >./styleguide/CNAME",
    "babel:lib": "NODE_ENV=production babel ./react/components --out-dir ./lib --extensions \".ts,.tsx\" --ignore '__tests__,*.spec.js' --ignore '*.md' --copy-files",
    "build:types": "tsc --emitDeclarationOnly",
    "babel:modules": "NODE_ENV=production babel ./react/modules --out-dir ./modules --ignore '__tests__,*.spec.js' --ignore '*.md' --copy-files",
    "clean:lib": "rm -rf lib",
    "clean:modules": "rm -rf modules",
    "compile": "run-s clean:lib clean:modules build:types babel:lib babel:modules",
    "prepublishOnly": "run-s compile deploy",
    "postreleasy": "vtex publish --public --verbose",
    "lint": "eslint react",
    "lint-fix": "eslint react --fix --max-warnings 0",
    "prettier-conflict-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "postinstall": "yarn --cwd react --ignore-scripts"
  },
  "files": [
    "lib/",
    "modules/",
    "codemod/"
  ],
  "homepage": "https://vtex.github.io/styleguide",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/prop-types": "^15.7.1",
    "@vtex/react-scripts": "1.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react-app-babel-7": "^4.0.2-0",
    "copyfiles": "^1.2.0",
    "css-modules-typescript-loader": "^2.0.4",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-vtex": "8.0.0",
    "eslint-config-vtex-react": "^3.0.3",
    "eslint-config-vtex-react-app": "^3.1.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "2",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-lodash": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-utils": "^1.3.1",
    "gh-pages": "^1.2.0",
    "husky": "^1.1.3",
    "jest-dom": "^3.0.0",
    "jscodeshift": "^0.5.0",
    "npm-run-all": "^4.1.3",
    "prettier-eslint": "^8.8.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-testing-library": "^5.4.4",
    "watch": "^1.0.2",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "focus-visible": "^4.1.5",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^3.2.0",
    "object-assign": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "promise": "^8.0.3",
    "raf": "^3.4.1",
    "react-color": "^2.17.0",
    "react-datepicker": "^2.3.0",
    "react-dev-utils": "^9.0.1",
    "react-docgen-displayname-handler": "^2.1.1",
    "react-number-format": "^4.0.6",
    "react-outside-click-handler": "^1.2.2",
    "react-overlays": "^1.1.2",
    "react-responsive-modal": "^2.0.1",
    "react-select": "^2.1.2",
    "react-styleguidist": "9.0.4",
    "react-virtualized": "^9.19.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "uuid": "^3.3.2",
    "vtex-tachyons": "^3.2.0",
    "webpack": "4.28.4",
    "whatwg-fetch": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prettier-conflict-check && npm run lint"
    }
  }
}
