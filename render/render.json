{
  "extensions": {
    "styleguide": {
      "component": "./Template.js",
      "theme": "./theme.css",
      "route": {
        "path": "/*path"
      }
    },
    "styleguide/home": {
      "component": "./Pages/Principles.js",
      "route": {
        "path": "/"
      }
    },
    "styleguide/principles": {
      "component": "./Pages/Principles.js",
      "route": {
        "path": "/principles"
      }
    },
    "styleguide/howto": {
      "component": "./Pages/HowTo.js",
      "route": {
        "path": "/howto/*"
      }
    },
    "styleguide/howto/first-steps": {
      "component": "./Pages/GettingStarted.js",
      "route": {
        "path": "/first-steps"
      }
    },
    "styleguide/howto/grid-system": {
      "component": "./Pages/GridSystem.js",
      "route": {
        "path": "/grid-system"
      }
    },
    "styleguide/element": {
      "component": "./Pages/ComponentPage.js",
      "route": {
        "path": "/elements/*element"
      }
    },
    "styleguide/element/example": {
      "component": "./Pages/DevPage.js",
      "route": {
        "path": "/example"
      }
    },
    "styleguide/component": {
      "component": "./Pages/ComponentPage.js",
      "route": {
        "path": "/components/*component"
      }
    },
    "styleguide/component/example": {
      "component": "./Pages/DevPage.js",
      "route": {
        "path": "/example"
      }
    },
    "styleguide/pattern": {
      "component": "./Pages/MarkdownPage.js",
      "route": {
        "path": "/patterns/:page"
      }
    }
  }
}
